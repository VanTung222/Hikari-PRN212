git <Application x:Class="HikariApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HikariApp"
             xmlns:converters="clr-namespace:HikariApp.Converters"
             StartupUri="CourseListPage.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <!-- Font Awesome Fonts -->
            <FontFamily x:Key="FontAwesomeSolid">pack://application:,,,/FontAwesome.Sharp;component/fonts/#Font Awesome 6 Free Solid</FontFamily>
            <FontFamily x:Key="FontAwesomeRegular">pack://application:,,,/FontAwesome.Sharp;component/fonts/#Font Awesome 6 Free Regular</FontFamily>
            <FontFamily x:Key="FontAwesomeBrands">pack://application:,,,/FontAwesome.Sharp;component/fonts/#Font Awesome 6 Brands Regular</FontFamily>

            <!-- Converters -->
            <converters:NotNullToVisibilityConverter x:Key="NotNullToVisibilityConverter"/>
            <converters:BooleanToStatusConverter x:Key="BooleanToStatusConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
